{
  "name": "fractionator",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tetrau.github.io/fractionator",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.55",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "bootstrap": "^4.5.2",
    "gh-pages": "^3.1.0",
    "pdfjs-dist": "^2.4.456",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "cp node_modules/pdfjs-dist/build/pdf.worker.min.js src/pdf.worker.min.data && react-scripts start",
    "build": "cp node_modules/pdfjs-dist/build/pdf.worker.min.js src/pdf.worker.min.data && react-scripts build",
    "test": "cp node_modules/pdfjs-dist/build/pdf.worker.min.js src/pdf.worker.min.data && react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "cp node_modules/pdfjs-dist/build/pdf.worker.min.js src/pdf.worker.min.data && gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}